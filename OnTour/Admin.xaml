<Window x:Class="OnTour.Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:OnTour"
        mc:Ignorable="d"
        Title="Administrador" Height="450" Width="800"
        Background="#FFF3E44B"
        Icon="imagenes/Ontour logo.png">
    <Grid Margin="0,0,0.333,344.667">
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" Margin="15 15 0 0" >
            <Button Uid="1" Width="160" Height="50" Content="Registros Clientes" Background="White" BorderBrush="{x:Null}" Foreground="Black" Click="Button_Click"/>
            <Button Uid="0" Width="160" Height="50" Content="Cotizaciones"  Background="White" BorderBrush="{x:Null}" Foreground="Black"  Click="Button_Click"/>
            
        </StackPanel>
        <Grid x:Name="CursorGrid" Width="150" Height="5" Background="Black" HorizontalAlignment="Left" Margin="21,56,0,357"/>

        <Grid>
            <Frame x:Name="FrameAdmin" Content="Frame" HorizontalAlignment="Left" Height="350" Margin="0,71,0,-0.333" VerticalAlignment="Top" Width="783"/>
        </Grid>
    </Grid>
    

</Window>
